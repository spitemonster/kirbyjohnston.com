---
import { getCollection } from "astro:content"
import WorkCard from "./WorkCard.astro"
const work = await getCollection('work')
---
{ work.map(w => {
	return (
		<WorkCard companyName={w.data.company_name} tags={w.data.tags ?? []} duration={w.data.duration} roles={w.data.roles ?? []} summary={w.data.summary} url={w.data.url} />
	)
})}
